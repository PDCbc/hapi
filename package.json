{
  "name": "scoop-queryengine-adapter",
  "version": "0.0.0",
  "description": "The SCOOP Query Engine Adapter",
  "main": "init.js",
  "scripts": {
    "start": "node init.js",
    "test": "sh runtest.sh",
    "test-coverage" : "mocha -c --recursive --require blanket -R html-cov > coverage.html",
    "test-no-coverage" : "mocha -c --recursive",
    "gen-docs" : "jsdoc -r ./lib -d doc/jsDoc; open doc/jsDoc/index.html" 
  },
  "config": {
    "blanket": {
      "pattern": ["lib", "lib/reports", "lib/resultManager", "lib/routes"],
      "data-cover-never" : [ "node_modules", "test"]
    }
  },
  "author": "Andrew Hobden <andrew+scoop@hoverbear.org>, Ryan Habibi <rphabibi@uvic.ca>",
  "license": "GPLv2",
  "private": true,
  "dependencies": {
    "async": "~0.9.0",
    "bcrypt": "~0.8.0",
    "blanket": "^1.1.7",
    "body-parser": "~1.10.0",
    "cli-color": "~0.3.2",
    "compression": "~1.2.2",
    "connect-ensure-login": "~0.1.1",
    "cookie-parser": "~1.3.1",
    "csurf": "~1.6.3",
    "express": "~4.10.6",
    "express-session": "~1.9.3",
    "hbs": "~2.7.0",
    "js-sha256": "^0.3.0",
    "lodash": "~2.4.1",
    "mocha": "^2.2.5",
    "mongoose": "~3.8.12",
    "morgan": "~1.5.0",
    "pem": "~1.4.4",
    "request": "~2.53.0"
  },
  "jshintConfig": {
    "latedef": "nofunc",
    "node": true
  }
}
